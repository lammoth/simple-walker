{
  "name": "simple-walker",
  "version": "0.1.0",
  "description": "simple-walker is a nodejs synchronous walker to search files inside dirs. It's specially designed to load dynamic files, like routes, modules, etc.",
  "main": "dist/walker",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lammoth/simple-walker.git"
  },
  "keywords": [
    "walk",
    "tree",
    "directory",
    "files",
    "walker",
    "path",
    "search",
    "match"
  ],
  "author": "Francisco Hidalgo <fjhidalgo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lammoth/simple-walker/issues"
  },
  "homepage": "https://github.com/lammoth/simple-walker#readme",
  "dependencies": {
    "minimist": "^1.2.0",
    "walk": "^2.3.9"
  }
}
